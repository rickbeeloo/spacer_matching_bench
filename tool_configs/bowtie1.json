{
  "name": "bowtie1",
  "output_file": "{output_dir}/bowtie1_output.sam",
  "parse_function_name": "parse_sam",
  "script_name": "bowtie1.sh",
  "mamba_env": "bowtie_env",
  "command": [
    "bowtie-build --threads {threads} {contigs_file} {output_dir}/bt1_contigs_index && bowtie --threads {threads} --seedmms -3 --no-unal -f --all -v {max_mismatches} -x {output_dir}/bt1_contigs_index {spacers_file} -S {output_dir}/bowtie1_output.sam"
  ]
}